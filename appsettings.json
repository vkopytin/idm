{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": {
      "Name": "File",
      "Args": {
        "path": "./Logs/ex_.log"
      }
    }
  },
  "JWT": {
    "SecretKey": "some-secret-key-with-long-description-on-prod",
    "Issuer": "https://localhost:5000",
    "Audience": "local-dev"
  },
  "AllowedHosts": "*",
  "Account": {
    "ApiUri": "https://localhost:3001",
    "ApiIp": ""
  },
  "Google": {
    "ClientId": "your-google-client-id.apps.googleusercontent.com",
    "ClientSecret": "your-google-client-secret",
    "RedirectUri": "https://localhost:5000/auth/googleLogin"
  }
}
